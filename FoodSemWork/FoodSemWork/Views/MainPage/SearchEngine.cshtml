@model IEnumerable<FoodSemWork.Models.TypesOfFood>

<div>
    <form class="form-inline" method="post" style="justify-content:center;flex-direction: column;">
        <input class="form-control mr-sm-2" type="search" name="searchString"  placeholder="Search" aria-label="Search" >
        <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
    </form>
</div>

<div class="container">

    @foreach(var item in Model)
    {
    <h4>
        @Html.DisplayFor(modelItem => item.TypeOfFood)
    </h4>
    <p>
        @Html.DisplayFor(modelItem => item.Receipt)
    </p>

    }

</div>